#include <iostream>
using namespace std;
// 입력받은 정수가 소수인지 판별
// 소수가 아니라면 -1 +1을 해가며 소수인지 판별
// 소수인 값이 나오는 경우 출력
int prime(int n) {  // 소수 판별 함수
    if (n < 2)      // n이 2보다 작은 경우 0을 리턴
        return 0;
    int count = 0;
    for (int i = 2; i <= n / i; i++) {  // 합성수임을 확인하기 위해 한쪽을 판별
        if (n % i == 0)
            return 0;
    }

    return 1;
}
void check(int n) {
    int i, j;
    i = n - 1;  // i는 점차 감소
    j = n + 1;  // j는 점차 증가

    if (prime(n) == 1) {  // 입력받은 n 자체가 소수인 경우
        cout << n << endl;
        return;
    }
    while (1) {  // 무한 루프로 i와 j값의 소수를 판별
        if (prime(i) == 1 && prime(j) == 1) {
            cout << i << " " << j << endl;
            break;
        } else if (prime(i) == 1) {
            cout << i << endl;
            break;
        } else if (prime(j) == 1) {
            cout << j << endl;
            break;
        }

        i--;
        j++;
    }
}
int main() {
    int N;
    cin >> N;
    int M[N];
    for (int i = 0; i < N; i++)
        cin >> M[i];

    for (int i = 0; i < N; i++)
        check(M[i]);
}
